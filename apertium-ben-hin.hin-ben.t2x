<rule comment="REGLA: NP OF NP (Eg: )">   <!--Write an example-->
      <pattern>
        <pattern-item n="noun"/>
        <pattern-item n="of_prep"/>
        <pattern-item n="noun"/>
      </pattern>
      <action>
<choose>
         <when>
          <test>
           <and>
            <equal>
             <clip pos="1" part="gen"/>
             <lit-tag v="m"/>
            </equal>
            <equal>
             <clip pos="1" part="nbr"/>
             <lit-tag v="sg"/>
            </equal>
            <equal>
             <clip pos="1" part="case"/>
             <lit-tag v="d"/>
            </equal>
          </and>
         </test>
        <let>
            <var n="lemvar"/>
                <lit v="{^kA$}"/>
        </let>
	</when>
         <when>
          <test>
           <and>
            <equal>
             <clip pos="1" part="gen"/>
             <lit-tag v="m"/>
            </equal>
            <equal>
             <clip pos="1" part="nbr"/>
             <lit-tag v="pl"/>
            </equal>
